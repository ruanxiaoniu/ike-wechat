page{
  width: 100%;
  height: 100%;
}
.table-scoller{
  border: 1px solid #DCDFE6;
}
.table{
  width: 100%;
  display: flex;
  white-space: nowrap;
}
.coll-nodata{
  border-bottom: 1px solid #DCDFE6;
}
.coll, .coll-nodata{
  /* border-top: 1px solid #DCDFE6; */
  border-right: 1px solid #DCDFE6;
  text-align: center;
  
}
.coll-nodata:last-child{
  border-bottom: 1px solid #DCDFE6;
}
.coll:last-child{
  border: none;
  /* border-bottom: 1px solid #DCDFE6; */
}
.th, .td{
  padding: 5px 10px;
}
.td{
  border-top: 1px solid #DCDFE6;
}
.empty{
  height: 100px;
  color: grey;
  text-align: center;
  display: flex;
  justify-content: center;
  /* border-top: 1px solid #DCDFE6; */
}
.empty-data{
  align-self: center;
}
